{
    "name": "n0sz/commonmark-marker-extension",
    "type": "commonmark-extension",
    "description": "A marker extension for CommonMark PHP implementation",
    "keywords": ["markdown","parser","commonmark"],
    "homepage": "https://github.com/noah1400/commonmark-marker-extension",
    "license": "MIT",
    "require": {
      "league/commonmark": "^2.3"
    },
    "autoload": {
      "psr-4": {
        "N0sz\\CommonMark\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "N0sz\\CommonMark\\Tests\\Unit\\": "tests/unit"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5.25",
      "phpstan/phpstan": "^1.8.6"
    },
    "scripts": {
        "test": "phpunit ./tests/unit"
    }
  }
